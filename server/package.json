{
  "name": "fixify-server",
  "version": "1.0.0",
  "description": "Backend server for Fixify - local service marketplace",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "node migrations/migrate.js",
    "migrate:dry": "node migrations/migrate.js --dry-run",
    "migrate:status": "node migrations/migrate.js --status",
    "migrate:rollback": "node migrations/migrate.js --rollback",
    "migrate:diagnose": "node migrations/diagnose.js",
    "backup": "node migrations/backup.js",
    "backup:list": "node migrations/backup.js --list",
    "backup:restore": "node migrations/backup.js --restore",
    "seed": "node scripts/seed-database.js",
    "seed:force": "node scripts/seed-database.js --force"
  },
  "keywords": [
    "mern",
    "marketplace",
    "services",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@clerk/clerk-sdk-node": "^5.1.6",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.40.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "geolib": "^3.3.4",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pino": "^9.8.0",
    "socket.io": "^4.8.1",
    "stripe": "^12.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
